<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<settings>
		<setting name="logImpl" value="STDOUT_LOGGING" />
	</settings>

	<!-- 实体类,简称 -设置别名 -->
	<typeAliases>
		<typeAlias alias="user" type="com.sadasen.account.entity.User" />
		<typeAlias alias="method" type="com.sadasen.account.entity.Method" />
		<typeAlias alias="consume" type="com.sadasen.account.entity.Consume" />
		<typeAlias alias="record" type="com.sadasen.account.entity.Record" />
	</typeAliases>
	
	<!-- 实体接口映射资源 -->
	<!--
		说明：如果xxMapper.xml配置文件放在和xxMapper.java统一目录下，mappers也可以省略，因为org.mybatis.spring.mapper.MapperFactoryBean默认会去查找与xxMapper.java相同目录和名称的xxMapper.xml
	<mappers>
		<mapper resource="com/tgb/mapper/userMapper.xml" />
	</mappers>
	-->

</configuration>  